<?xml version='1.0' encoding='utf-8'?>
<widget id="com.verint.xm.sdk.dhofarCordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BlinkIDScanner">
        <param name="android-package" value="com.microblink.blinkid.plugins.cordova.BlinkIDScanner" />
    </feature>
    <name>HelloCordova</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="android-minSdkVersion" value="31" />
    <preference name="android-targetSdkVersion" value="31" />
</widget>
